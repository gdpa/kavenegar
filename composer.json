{
    "name": "gdpa/kavenegar",
    "description": "Kavenegar Notifications driver",
    "homepage": "https://github.com/gdpa/kavenegar",
    "license": "MIT",
    "authors": [
        {
            "name": "Morteza Poussaneh",
            "email": "morteza.poussaneh@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "guzzlehttp/guzzle": "~6.3.1|^7.0.1",
        "illuminate/notifications": "5.8.*|^6.0",
        "illuminate/support": "5.8.*|^6.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.0",
        "orchestra/testbench": "~3.8|~4.0"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\Kavenegar\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\Kavenegar\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
